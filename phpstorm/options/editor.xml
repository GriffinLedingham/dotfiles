<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Dank Mono" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Bold" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.0" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>